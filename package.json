    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/node": "^20.6.4",
    "commitizen": "^4.3.0",
    "eslint": "^8.50.0",
    "fast-glob": "^3.3.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
